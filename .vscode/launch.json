    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Express",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\src\\server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Tests",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\node_modules\\jest\\bin\\jest.js",
            "args": [
                "--testMatch",
                "\"<rootDir>/test/e2e/test/*.test.js\"",
            ],
        },
    ]
}